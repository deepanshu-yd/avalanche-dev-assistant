{
  "name": "avalanche-assistant-server",
  "version": "0.2.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "ingest:fetch": "ts-node src/rag/ingest.ts --mode=fetch",
    "ingest:local": "ts-node src/rag/ingest.ts --mode=local",
    "cleanDocs": "ts-node src/rag/clean.ts",
    "chunk": "ts-node src/rag/chunk.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.7.2",
    "cheerio": "1.0.0-rc.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "gray-matter": "^4.0.3",
    "marked": "^12.0.2",
    "normalize-url": "^6.1.0",
    "openai": "^5.12.2",
    "p-limit": "^3.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
